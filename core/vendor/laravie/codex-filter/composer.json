{
    "name": "laravie/codex-filter",
    "description": "Content filtering for Laravie Codex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\Filter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0.0 <8.3.0",
        "igorw/get-in": "^1.0",
        "laravie/codex-common": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.4.4",
        "php-http/guzzle7-adapter": "^1.0",
        "phpstan/phpstan": "^1.10.5",
        "phpunit/phpunit": "^9.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable"
}
